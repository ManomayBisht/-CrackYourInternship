class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count = 0;
        int candid = nums[0];

        for(int  i=0; i<  nums.size(); i++) {
            if (count == 0) {
                candid = nums[i];
            }
            if(nums[i] == candid) {
                count++;
            } else {
                count--;
            }
        }
        return candid;   
    }
};
